import {FC, JSX} from "react";
import go_home from "../../assets/images/button_bg.webp"
import {useTranslation} from "react-i18next";
import {NavigateFunction, useNavigate} from "react-router-dom";


const NotFoundContainer: FC = (): JSX.Element => {

    const {t} = useTranslation();
    const navigate: NavigateFunction = useNavigate();
    const audio_ref = useRef(new Audio("src/assets/audio/go_home.mp3"));

    return <section className={"not_found"}>


        <h1>{t("not_found_text")}</h1>


        <figure onClick={() => {
            navigate("/")
        }}>
            <img src={go_home} alt="go_home"/>

            <h1>go home</h1>
        </figure>

    </section>
}

export default NotFoundContainer;